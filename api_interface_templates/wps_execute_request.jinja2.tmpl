<wps:Execute
	xmlns:wps="http://www.opengis.net/wps/2.0"
	xmlns:ows="http://www.opengis.net/ows/2.0"
	xmlns:xlink="http://www.w3.org/1999/xlink"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.opengis.net/wps/2.0 ../wps.xsd" service="WPS"
	   version="2.0.0" response="document" mode="sync">
    {%- for param in data.inputs.keys() %}
    <wps:Input id="{{ param }}">
    <wps:Data>
    <wps:LiteralValue>{{ data.inputs[param] }}</wps:LiteralValue>
    </wps:Data>
    </wps:Input>
    {%- endfor %}
	<ows:Identifier>{{ data.job_type }}</ows:Identifier>
	<wps:Input id="username">
		<wps:Data>
			<wps:LiteralValue>{{ data.username }}</wps:LiteralValue>
		</wps:Data>
	</wps:Input>
    <wps:Input id="queue">
		<wps:Data>
			<wps:LiteralValue>{{ data.resource }}</wps:LiteralValue>
		</wps:Data>
	</wps:Input>
	<wps:Output id="result" transmission="value"/>
</wps:Execute>